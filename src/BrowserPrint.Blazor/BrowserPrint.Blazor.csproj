<Project Sdk="Microsoft.NET.Sdk.Razor">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<RootNamespace>BrowserPrint</RootNamespace>
        <Description>Zebra Browser Print support for .NET Blazor</Description>
        <PackageTags>Zebra BrowserPrint Browser Print Blazor</PackageTags>
        <PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components" />
		<PackageReference Include="Microsoft.JSInterop" />
		<PackageReference Include="Neon.Blazor" />
		<PackageReference Include="Neon.Common" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="wwwroot\js\interop.js" />
	</ItemGroup>

    <ItemGroup>
        <None Include="..\..\LICENSE" Pack="true" PackagePath="" />
        <None Include="..\..\README.md" Pack="true" PackagePath="" />
    </ItemGroup>

    
</Project>
